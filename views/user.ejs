<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>demo socket io</title>
    <link rel="stylesheet" type="text/css" href="/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="/css/datepicker.css?v=1">
    <script src="/js/jquery.min.js" type="text/javascript"></script>
    <script src="/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="/js/datepicker_v2.js" type="text/javascript"></script>
    <script src="/socket.io/socket.io.js"></script>
</head>
<body>
    <div class="container">
        <div class="row text-center">
            <h2>Demo send real time</h2>
            <button class="btn btn-primary">Refresh page</button>
        </div>
    </div>
    <script type="text/javascript">
        $(function(){
            var socket = io.connect('http://localhost:3000/');
            $('button').click(function(){
                socket.emit('reviceFromClient',{frdate: null, tdate: null});
            });

            setInterval(function(){
                socket.emit('reviceFromClient',{frdate: null, tdate: null});
            },10000);
        });
    </script>
</body>
</html>